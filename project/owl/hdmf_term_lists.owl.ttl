@prefix IAO: <http://purl.obolibrary.org/obo/IAO_> .
@prefix PATO: <http://purl.obolibrary.org/obo/PATO_> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix hdmf_term_lists: <https://w3id.org/hdmf-dev/hdmf-term-lists/> .
@prefix linkml: <https://w3id.org/linkml/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://w3id.org/hdmf-dev/hdmf-term-lists> a owl:Ontology ;
    rdfs:label "hdmf-term-lists" ;
    IAO:0000700 hdmf_term_lists:NamedThing,
        hdmf_term_lists:SubjectCollection ;
    dcterms:license "MIT" ;
    dcterms:title "hdmf-term-lists" ;
    rdfs:seeAlso "https://hdmf-dev.github.io/hdmf-term-lists" ;
    linkml:generation_date "2022-12-13T10:36:03" ;
    linkml:metamodel_version "1.7.0" ;
    linkml:source_file "hdmf_term_lists.yaml" ;
    linkml:source_file_date "2022-12-13T10:30:04" ;
    linkml:source_file_size 2244 .

linkml:SubsetDefinition a owl:Class ;
    rdfs:label "subset_definition" .

linkml:TypeDefinition a owl:Class ;
    rdfs:label "type_definition" .

linkml:topValue a owl:DatatypeProperty ;
    rdfs:label "value" .

hdmf_term_lists:SubjectCollection a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "SubjectCollection" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom hdmf_term_lists:Subject ;
            owl:onProperty hdmf_term_lists:entries ] ;
    skos:definition "A holder for Subject objects" .

hdmf_term_lists:age_in_years a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "age_in_years" ;
    rdfs:range linkml:Integer ;
    skos:definition "Number of years since birth" .

hdmf_term_lists:entries a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "entries" ;
    rdfs:range hdmf_term_lists:Subject .

hdmf_term_lists:vital_status a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "vital_status" ;
    rdfs:range hdmf_term_lists:PersonStatus ;
    skos:definition "living or dead status" .

PATO:0001421 a owl:Class,
        hdmf_term_lists:PersonStatus ;
    rdfs:label "ALIVE" .

PATO:0001422 a owl:Class,
        hdmf_term_lists:PersonStatus ;
    rdfs:label "DEAD" .

schema:birthDate a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "birth_date" ;
    rdfs:range linkml:Date ;
    skos:definition "Date on which a person is born" ;
    skos:exactMatch schema:birthDate .

schema:description a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "description" ;
    rdfs:range linkml:String ;
    skos:definition "A human-readable description for a thing" ;
    skos:exactMatch schema:description .

schema:email a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "primary_email" ;
    rdfs:range linkml:String ;
    skos:definition "The main email address of a person" ;
    skos:exactMatch schema:email .

schema:identifier a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "id" ;
    rdfs:range linkml:Uriorcurie ;
    skos:definition "A unique identifier for a thing" ;
    skos:exactMatch schema:identifier .

schema:name a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "name" ;
    rdfs:range linkml:String ;
    skos:definition "A human-readable name for a thing" ;
    skos:exactMatch schema:name .

hdmf_term_lists:NamedThing a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "NamedThing" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty schema:description ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty schema:name ],
        [ a owl:Restriction ;
            owl:onClass linkml:Uriorcurie ;
            owl:onProperty schema:identifier ;
            owl:qualifiedCardinality 1 ] ;
    skos:definition "A generic grouping for any identifiable entity" ;
    skos:exactMatch schema:Thing .

<https://w3id.org/hdmf-dev/hdmf-term-lists/PersonStatus#UNKNOWN> a owl:Class,
        hdmf_term_lists:PersonStatus ;
    rdfs:label "UNKNOWN" .

hdmf_term_lists:Subject a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Subject" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Date ;
            owl:onProperty schema:birthDate ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass hdmf_term_lists:PersonStatus ;
            owl:onProperty hdmf_term_lists:vital_status ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty schema:email ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Integer ;
            owl:onProperty hdmf_term_lists:age_in_years ],
        hdmf_term_lists:NamedThing ;
    skos:definition "Represents a Subject" .

linkml:ClassDefinition a owl:Class ;
    rdfs:label "class_definition" .

hdmf_term_lists:PersonStatus a owl:Class,
        linkml:EnumDefinition ;
    rdfs:label "PersonStatus" ;
    owl:unionOf ( PATO:0001421 PATO:0001422 <https://w3id.org/hdmf-dev/hdmf-term-lists/PersonStatus#UNKNOWN> ) ;
    linkml:permissible_values PATO:0001421,
        PATO:0001422,
        <https://w3id.org/hdmf-dev/hdmf-term-lists/PersonStatus#UNKNOWN> .

linkml:SlotDefinition a owl:Class ;
    rdfs:label "slot_definition" .


